{
  "name": "@memsched/memsched",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev --open",
    "build": "pnpm -C ../blog build && vite build",
    "build:e2e": "vite build --mode testing",
    "preview": "wrangler pages dev --port 5173",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "fix": "pnpm run check && eslint --fix --cache . && pnpm run format",
    "format": "prettier --write .",
    "lint": "pnpm run check && prettier --check . && eslint .",
    "test:unit": "vitest run",
    "test": "pnpm run test:unit -- --run && pnpm run test:e2e",
    "test:e2e": "playwright test",
    "db:push": "drizzle-kit push",
    "db:generate": "DATABASE_URL=file:local.db drizzle-kit generate",
    "db:migrate": "DATABASE_URL=file:local.db drizzle-kit migrate",
    "db:studio": "DATABASE_URL=file:local.db drizzle-kit studio",
    "deploy": "pnpm run build && wrangler pages deploy",
    "cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
    "d1:migrate": "wrangler d1 migrations apply memsched-db --local",
    "prod:d1:create": "wrangler d1 create memsched-db --remote",
    "prod:kv:create": "wrangler kv namespace create memsched-kv",
    "prod:d1:migrate": "wrangler d1 migrations apply memsched-db --remote --env production",
    "prod:d1:execute": "wrangler d1 execute memsched-db --remote --env production"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260103.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@lucide/svelte": "^0.562.0",
    "@memsched/ui": "workspace:*",
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/enhanced-img": "^0.9.2",
    "@sveltejs/kit": "^2.49.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/culori": "^4.0.1",
    "autoprefixer": "^10.4.23",
    "bits-ui": "^1.8.0",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-svelte": "^3.13.1",
    "formsnap": "^2.0.1",
    "globals": "^15.15.0",
    "lucide-svelte": "^0.483.0",
    "neverthrow": "^8.2.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-icons-pack": "^3.1.3",
    "svelte-markdown": "^0.4.1",
    "svelte-seo": "^2.0.0",
    "sveltekit-rate-limiter": "^0.7.0",
    "sveltekit-superforms": "^2.29.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "wrangler": "^4.54.0",
    "zod": "^4.3.5"
  },
  "dependencies": {
    "@cf-wasm/photon": "^0.3.4",
    "@cf-wasm/resvg": "^0.3.3",
    "@libsql/client": "^0.14.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "arctic": "^3.7.0",
    "culori": "^4.0.2",
    "date-fns": "^4.1.0",
    "domhandler": "^5.0.3",
    "drizzle-orm": "^0.41.0",
    "highlight.js": "^11.11.1",
    "html-react-parser": "5.2.11",
    "htmlparser2": "^10.0.0",
    "mode-watcher": "^1.1.0",
    "satori": "^0.15.2",
    "stripe": "^17.7.0",
    "svelte-french-toast": "^1.2.0",
    "uuid": "^13.0.0"
  },
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": "^24.0.0"
  }
}

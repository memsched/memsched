{
  "name": "memsched-monorepo",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm -r run dev",
    "build": "pnpm --filter memsched build",
    "preview": "pnpm --filter memsched preview",
    "check": "pnpm -r run check",
    "fix": "pnpm -r run fix",
    "format": "pnpm -r run format",
    "lint": "prettier --check . && pnpm -r run lint",
    "test": "pnpm -r run test",
    "test:unit": "pnpm -r run test:unit",
    "test:e2e": "pnpm -r run test:e2e",
    "deploy": "pnpm --filter memsched deploy"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11"
  },
  "packageManager": "pnpm@10.3.0",
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "esbuild@<=0.24.2": ">=0.25.0",
      "@babel/runtime@<7.26.10": ">=7.26.10"
    }
  },
  "engines": {
    "node": "^22.12.0"
  }
}

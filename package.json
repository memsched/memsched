{
  "name": "memsched-monorepo",
  "private": true,
  "version": "0.0.1",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "bun run --filter '*' dev",
    "build": "bun run --filter '@memsched/memsched' build",
    "preview": "bun run --filter '@memsched/memsched' preview",
    "format": "bunx prettier --write . && bun run --filter '*' format",
    "lint": "bun run --filter '@memsched/memsched' lint",
    "fix": "bun run --filter '@memsched/memsched' fix",
    "deploy": "bun run --filter '@memsched/memsched' deploy"
  },
  "devDependencies": {
    "prettier": "^3.8.1"
  },
  "overrides": {
    "cookie": ">=0.7.0",
    "esbuild": ">=0.25.0",
    "@babel/runtime": ">=7.26.10",
    "brace-expansion": ">=2.0.2",
    "diff": ">=8.0.3",
    "undici": ">=7.18.2"
  },
  "engines": {
    "node": "^24.0.0"
  }
}
